{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Set-Cookie",
					"value": "cross-site-cookie=whatever; SameSite=None; Secure"
				},
				{
					"key": "Permissions-Policy",
					"value": "ch-ua-form-factor"
				}
			]
		}
	],
	"routes": [
		{
			"src": "/.*",
			"dest": "/"
		}
	]
}